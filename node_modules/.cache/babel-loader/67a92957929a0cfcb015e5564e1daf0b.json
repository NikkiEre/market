sFocus = false;\n        },\n        showMenu: function() {\n            this.isShow = !this.isShow;\n        },\n        moveToLoginOrReg: function() {\n            if(this.isAuth) {\n                const answer = confirm(\"Do you want to logout?\");\n                if(answer) {\n                    store.commit(\"logoutUser\");\n                }\n            } else {\n                this.$router.push(\"/register\");\n            }\n        }\n    },\n}\n</script>\n\n<style lang=\"less\" scoped>\n    @import \"../../style/variables.less\";\n\n    .header {\n        background: @color-black;\n        padding-top: 18px;\n        padding-bottom: 18px;\n\n        &__container {\n            display: flex;\n            justify-content: space-between;\n            align-items: center;\n            position: relative;\n        }\n\n        &__count {\n            display: inline-flex;\n            background: @color-pink;\n            border-radius: 50px;\n            position: absolute;\n            height: 20px;\n            width: 20px;\n            align-items: center;\n            justify-content: center;\n            color: @color-white;\n            top: -5px;\n            right: -5px;\n        }\n\n        &__logo {\n            transition: all 0.5s;\n\n            &:hover {\n                opacity: 0.5;\n            }\n\n            &:active {\n                opacity: 0.7;\n            }\n        }\n\n        &__link {\n            transition: all 0.5s;\n            cursor: pointer;\n            position: relative;\n\n            & svg path{\n                transition: all 0.5s;\n            }\n\n            &:active {\n                opacity: 0.5;\n            }\n\n            &:hover svg path{\n                fill: @color-pink;\n            }\n\n            &_ava {\n                height: 30px;\n                width: 30px;\n                align-self: center;\n                display: inline-flex;\n                align-items: center;\n                background: @color-white;\n                border-radius: 30px;\n                justify-content: center;\n                color: @color-pink;\n\n                &:hover {\n                    background: @color-pink;\n                    color: @color-white;\n                }\n            }\n\n            &_auth,\n            &_cart {\n                @media (min-width: @min-mobile-width) and (max-width: @max-mobile-width) {\n                    display: none;\n                }\n            }\n        }\n\n        &__button {\n            background: none;\n            border: none;\n        }\n\n    }\n\n    nav {\n        display: flex;\n        align-items: baseline;\n        gap: 40px;\n    }\n\n</style>"]},"metadata":{},"sourceType":"module"}